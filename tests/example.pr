@define(project) -> ["tests/"]
    @struct(
        "tests/",
        "docs/",
        "project",
    )
    
    future = {from __future__ import annotations}
    imports = {
        from argparse import ArgumentParser
        from project import __version__, __author__
    }

    meta = {
        __version__ = "0.1.0"
        __author__ = "andy"
    }

	@filetype(py)
	-> project(__init__) = {
		meta
	}

	@filetype(py)
	-> project(__main__) = {
		imports
	}